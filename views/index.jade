extends layout

block content	
    h4 Chilli farm overview

    table.table.table-striped.table-responsive
        tbody
            tr
                td Soil
                td #{soilTemp} °C
            tr
                td Air
                td #{airTemp} °C, #{airHum}%
            tr
                td Last Update
                td #{lastUpdate}
            tr
                td System
                td #{sysUpTime}

    img.img-responsive.center-block(src="img/latestPhoto.jpg")
